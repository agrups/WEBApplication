<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainSystemWindow">
    <children>
        <HBox layoutX="13.0" layoutY="37.0" prefHeight="349.0" prefWidth="575.0">
            <children>
                <AnchorPane prefHeight="349.0" prefWidth="261.0">
                    <children>
                        <Button fx:id="updateYourInfoBtn" layoutX="18.0" layoutY="186.0" mnemonicParsing="false"
                                onAction="#updateYourInfoForm" prefHeight="46.0" prefWidth="248.0"
                                text="Update Your Info"/>
                        <Button fx:id="incomeExpensesBtn" layoutX="19.0" layoutY="113.0" mnemonicParsing="false"
                                onAction="#manageIncomeExpensesForm" prefHeight="46.0" prefWidth="248.0"
                                text="Income/Expenses"/>
                        <Button fx:id="manageCategoriesBtn" layoutX="18.0" layoutY="30.0" mnemonicParsing="false"
                                onAction="#manageCategoriesForm" prefHeight="46.0" prefWidth="248.0"
                                text="Manage Categories"/>
                        <Button fx:id="exitBtn" layoutX="15.0" layoutY="275.0" mnemonicParsing="false" onAction="#exit"
                                prefHeight="46.0" prefWidth="255.0" text="Exit"/>
                    </children>
                </AnchorPane>
                <AnchorPane prefHeight="200.0" prefWidth="200.0">
                    <children>
                        <Button fx:id="listUsersBtn" layoutX="30.0" layoutY="184.0" mnemonicParsing="false"
                                onAction="#listUsersForm" prefHeight="46.0" prefWidth="255.0"
                                text="List All Company Users"/>
                        <Button fx:id="balanceBtn" layoutX="30.0" layoutY="111.0" mnemonicParsing="false"
                                onAction="#showBalance" prefHeight="46.0" prefWidth="255.0" text="Balance"/>
                        <Button fx:id="systemInfoBtn" layoutX="29.0" layoutY="32.0" mnemonicParsing="false"
                                onAction="#showSystemInfo" prefHeight="46.0" prefWidth="248.0" text="System Info"/>
                    </children>
                </AnchorPane>
            </children>
        </HBox>
        <MenuBar prefHeight="25.0" prefWidth="600.0">
            <menus>
            </menus>
        </MenuBar>
    </children>
</AnchorPane>
